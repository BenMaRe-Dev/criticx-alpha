<h1>List of Companies</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Country</th>
    <th></th>
  </tr>
  <% @companies.each do |company| %>
  <tr>
    <td><%= company.name %></td>
    <td><%= company.country %></td>
    <td><%= link_to "Show", company_path(company) %></td>
  </tr>
  <% end %>
</table>