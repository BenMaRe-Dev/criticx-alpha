<h1>All the Games</h1>
<%= link_to "New Game", new_game_path %>

<table>
  <tr>
    <th>Name</th>
    <th>Rating</th>
    <th></th>
  </tr>
  <% @games.each do |game| %>
  <tr>
    <td><%= game.name %></td>
    <td><%= game.rating %></td>
    <td><%= link_to "Show", game_path(game) %></td>
  </tr>
  <% end %>
</table>